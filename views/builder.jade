extends layout
include mixins/builder-row

block content
  body
    header
      include ./nav.jade

    include ./login.jade
    div.builder
      h5 Questions | Answers
      each val, index in new Array(1)
        +builder-row(index + 1)
      button.save-changes-button(onclick="onSumbitFaq()") Update

    script(src='https://www.gstatic.com/firebasejs/3.0.5/firebase.js')
    script(src='https://www.gstatic.com/firebasejs/3.0.5/firebase-app.js')
    script(src='https://www.gstatic.com/firebasejs/3.0.5/firebase-auth.js')
    script(src='https://www.gstatic.com/firebasejs/3.0.5/firebase-database.js')
    script(src='javascripts/firebase.js')
    script(src="javascripts/builderRow.js")
    script(src="javascripts/builder.js")
